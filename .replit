modules = ["python-3.11", "nix", "postgresql-16", "dotnet-7.0"]

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "python main.py"]
deploymentTarget = "cloudrun"

[deployment.cloudrun]
memory = "512Mi"
region = "us-central1"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flask Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Create Tables"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Analyze Curriculum"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Test Import Section"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Import Curriculum"

