args = "Verify C2.1"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Import ICD2O Curriculum"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Update English Translations"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Apply Migrations"

[[workflows.workflow.tasks]]
